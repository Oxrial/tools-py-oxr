{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "App",
			"type": "node-terminal",
			"request": "launch",
			"command": "pnpm dev",
			"cwd": "${workspaceFolder}/app"
		},
		{
			"name": "Server",
			"type": "debugpy",
			"request": "launch",
			"module": "tool_oxr.main",
			"cwd": "${workspaceFolder}/server/src",
			"console": "integratedTerminal"
		}
	],
	"compounds": [
		{
			"name": "Dev All",
			"configurations": ["App", "Server"]
		}
	]
}
